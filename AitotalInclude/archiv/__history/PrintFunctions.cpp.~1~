// ---------------------------------------------------------------------------
#pragma hdrstop
#include "PrintFunctions.h"
// ---------------------------------------------------------------------------
#pragma package(smart_init)
// ---------------------------------------------------------------------------
extern void toLog(const char* line); // ф-ция логирования из файла формы

// ---------------------------------------------------------------------------
void PrintString(const UString& s) {
	toLog((LPCSTR)GetOemString(s));
}

// ---------------------------------------------------------------------------
void PrintString(const AString& s) {
	toLog((LPCSTR)s);
}

// ---------------------------------------------------------------------------
void PrintNewLine() {
}

// ---------------------------------------------------------------------------
void PrintStringLn(const AString& s) {
	toLog(s);
}

// ---------------------------------------------------------------------------
void PrintError(const AString& s) {
	PrintString(s);
}
// ---------------------------------------------------------------------------
